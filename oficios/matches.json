[
    {
        "name": "matches",
        "type": "base",
        "schema": [
            {
                "name": "profesional_id",
                "type": "text",
                "required": true,
                "options": {
                    "max": 20
                }
            },
            {
                "name": "token_calificacion",
                "type": "text",
                "required": true,
                "options": {
                    "min": 3,
                    "max": 3
                }
            },
            {
                "name": "estado",
                "type": "select",
                "required": false,
                "options": {
                    "maxSelect": 1,
                    "values": [
                        "contacto_creado",
                        "calificado",
                        "expirado"
                    ]
                }
            },
            {
                "name": "expira_en",
                "type": "date",
                "required": true
            },
            {
                "name": "calificacion_id",
                "type": "text",
                "required": false,
                "options": {
                    "max": 15
                }
            }
        ],
        "listRule": "",
        "viewRule": "",
        "createRule": "",
        "updateRule": "estado = 'contacto_creado'",
        "deleteRule": null
    }
]